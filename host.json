{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetrics": true,
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "queues": {
      "maxDequeueCount": 1,
      "visibilityTimeout": "00:00:10"
    }
  }
}